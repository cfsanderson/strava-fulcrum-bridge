        "form_schema": {
          "name": "GeoPhoto",
          "description": "This app takes a single photo and uses the location metadata on that photo to set it's location in Fulcrum.",
          "record_title_key": "d750",
          "title_field_keys": [
            "d750",
            "6513"
          ],
          "status_field": {
            "type": "StatusField",
            "label": "Status",
            "key": "@status",
            "data_name": "status",
            "default_value": "",
            "enabled": false,
            "read_only": false,
            "hidden": false,
            "description": "",
            "choices": [

            ],
            "required": false,
            "disabled": false,
            "default_previous_value": false
          },
          "auto_assign": false,
          "hidden_on_dashboard": false,
          "geometry_types": [
            "Point",
            "MultiPoint",
            "LineString",
            "MultiLineString",
            "Polygon",
            "MultiPolygon"
          ],
          "geometry_required": false,
          "script": "function setPhotoLocation(event) {\n  SETLABEL('photo_metadata', event ? [\n    `Latitude: ${event.value.latitude}`,\n    `Longitude: ${event.value.longitude}`,\n    `Altitude: ${event.value.altitude}`,\n    `Id: ${event.value.id}`,\n    `Size: ${event.value.size}`,\n    `Accuracy: ${event.value.accuracy}`,\n    `Direction: ${event.value.direction}`,\n    `Orientation: ${event.value.orientation}`,\n    `Width: ${event.value.width}.`,\n    `Height: ${event.value.height}`,\n    `Timestamp: ${event.value.timestamp}`,\n  ].join('\\n') : 'No photo metadata available');\n  SETLOCATION(event.value.latitude, event.value.longitude);\n  SETVALUE('timestamp', event.value.timestamp);\n}\n\nfunction validateGeotags(event) {\n  if (!event.value.latitude || !event.value.longitude) {\n    INVALID('This photo is NOT geotagged. Enable photo geotagging on your device and try again.');\n  }\n}\n\nfunction startup() {\n  setPhotoLocation();\n  if (ISMOBILE()) {\n    SETTIMEOUT(setPhotoLocation, 10);\n    SETINTERVAL(setPhotoLocation, 1000);\n  }\n}\n\nON('add-photo', 'photo', setPhotoLocation);\n\nON('load-record', function () {\n  DATANAMES('PhotoField').forEach(function(dataName) {\n    ON('add-photo', dataName, validateGeotags);\n  });\n});\n",
